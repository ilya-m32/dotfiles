[push]
	default = current
[pulll]
	default = current
[pull]
	default = current
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = master
[commit]
	gpgsign = false
[core]
	editor = nvim
	pager = delta
[interactive]
	diffFilter = delta --color-only
[delta]
	navigate = true
	true-color = always
	syntax-theme = "ansi"
	color-only = true
[merge]
	conflictStyle = zdiff3
